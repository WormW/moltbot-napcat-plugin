{
  "name": "@moltbot/napcat",
  "version": "2026.1.27-beta.1",
  "type": "module",
  "description": "Moltbot Napcat OneBot v11 channel plugin",
  "moltbot": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "napcat",
      "label": "Napcat",
      "selectionLabel": "Napcat (OneBot v11)",
