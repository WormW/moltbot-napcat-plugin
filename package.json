      "docsLabel": "napcat",
      "blurb": "OneBot v11 via Napcat; private messages only.",
      "order": 85,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@moltbot/napcat",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "clawdbot": {
    "extensions": [
      "./index.ts"
    ]
  },
  "dependencies": {
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {},
  "devDependencies": {}
